{
  "name": "vscode-font-switch",
  "displayName": "Font Switch",
  "description": "Switch editor font family with live preview",
  "version": "1.0.0",
  "publisher": "FatehUllah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ufateh/vscode-font-switch/issues"
  },
  "homepage": "https://github.com/ufateh/vscode-font-switch",
  "repository": {
    "type": "git",
    "url": "https://github.com/ufateh/vscode-font-switch.git"
  },
  "icon": "images/logo.png",
  "keywords": [
    "editor",
    "font",
    "font family",
    "typography",
    "theme"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "editorFont.set",
        "title": "Font Switch: Set editor font"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.74.0",
    "mocha": "^10.7.0",
    "typescript": "^5.5.4"
  }
}
